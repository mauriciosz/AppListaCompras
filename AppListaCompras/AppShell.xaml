<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="AppListaCompras.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:AppListaCompras.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="AppListaCompras">
    <TabBar >
        <ShellContent
            Title="Listas"
            ContentTemplate="{DataTemplate local:ListToBuyPage}"
            Route="ListToBuy"
            Icon="icon_lista.png"/>   
    
        <ShellContent
            Title="Perfil"
            ContentTemplate="{DataTemplate local:ProfilePage}"
            Route="Profile" 
            Icon="icon_perfil.png"/>
    </TabBar>
</Shell>
